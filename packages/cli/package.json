    "build:es5": "tsc --build",
    "build": "run-p build:*",
    "test:jest": "jest",
    "test": "run-s build test:jest",
    "cli:run": "node ./es5/bin/cli.js",
    "cli:test": "run-s build cli:run"
  },
  "dependencies": {
    "@sde/types": "0.0.1",
    "@types/node": "^13.9.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
