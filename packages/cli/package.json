{
  "name": "@sde/team-cli",
  "version": "0.0.1",
  "main": "es5",
  "types": "types",
  "typings": "src",
  "bin": {
    "sde-team": "./es5/bin/cli"
  },
  "scripts": {
    "clean:es5": "rimraf es5",
    "clean:types": "rimraf types",
    "clean:cache": "rimraf *.tsbuildinfo",
