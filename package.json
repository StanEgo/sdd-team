{
    "private": true,
    "version": "0.0.1",
    "workspaces": [
        "docs",
        "packages/*",
        "../typescript/packages/*"
    ],
    "scripts": {
        "docs:dev": "yarn --cwd docs/ run dev",
        "docs:publish": "yarn --cwd docs/ run publish",
        "clean:es5": "rimraf packages/*/es5",
        "clean:types": "rimraf packages/*/types",
        "clean:tsc": "rimraf **/*.tsbuildinfo",
        "clean": "run-p clean:*",
        "ts:build": "tsc --build",
        "ts:watch": "tsc --build --watch",
        "ts:dev": "tsc --build tsconfig.dev.json --watch",
        "dev": "run-s clean ts:dev",
        "sde:test": "sde"
    },
    "dependencies": {
        "@sde/cli": "0.0.1"
    }
}